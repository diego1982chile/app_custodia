<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MyActivity$PlaceholderFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/headhead"
        android:layout_gravity="left|top"
        android:id="@+id/imageViewFragmen1" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/formato_headerindex"
        android:gravity="right"
        android:background="#33b5e5"
        android:paddingLeft="180dp"
        android:paddingRight="20dp"
        android:layout_below="@+id/imageView"
        android:layout_gravity="right" />

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    <test3.ncxchile.cl.widgets.CustomScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:id="@+id/scrollView1">
    <!--ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_marginTop="10dp"
        android:layout_below="@id/imageView1"-->

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="20dp"
            android:paddingBottom="70dp"
            android:layout_centerHorizontal="true">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/autoridad_solicita"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:showDividers="middle"
                android:layout_marginBottom="14dp"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:id="@+id/tableRow33">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/numero"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView333" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_00_orden"
                    android:inputType="number"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_weight="0.51"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/autoridad_solicita"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:id="@+id/tableRow">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/rut_aut"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView2" />

                <test3.ncxchile.cl.widgets.RutEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_01_rut"
                    android:maxLength="9"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_weight="0.51"
                    android:hint="Ingresar rut sin puntos ni guiones"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow"
                android:layout_alignLeft="@+id/tableRow"
                android:layout_alignStart="@+id/tableRow"
                android:id="@+id/tableRow2">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/nombre_aut"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView3" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_02_nombre"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:maxLength="35"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow"
                android:layout_alignLeft="@+id/tableRow"
                android:layout_alignStart="@+id/tableRow"
                android:id="@+id/tableRow2_1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/apellido_pat_aut"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView3_1" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_02_apellidopaterno"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:maxLength="35"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow"
                android:layout_alignLeft="@+id/tableRow"
                android:layout_alignStart="@+id/tableRow"
                android:id="@+id/tableRow2_2">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/apellido_mat_aut"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView3_2" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_02_apellidomaterno"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:maxLength="35"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow"
                android:layout_alignLeft="@+id/tableRow"
                android:layout_alignStart="@+id/tableRow"
                android:id="@+id/tableRow2_3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/fono_aut"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView3_3" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_02_telefonos"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:hint="(+56 2) 11111111"
                    android:imeOptions="actionNext"
                    android:digits="+0123456789"
                    android:maxLength="35"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow"
                android:layout_alignLeft="@+id/tableRow"
                android:layout_alignStart="@+id/tableRow"
                android:id="@+id/tableRow2_4">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/correo_aut"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView3_4" />

                <test3.ncxchile.cl.widgets.CorreoEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_02_correos"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:maxLength="35"
                    android:hint="@string/prompt_correo"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow4"
                android:layout_alignLeft="@+id/tableRow4"
                android:id="@+id/tableRow5">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/institucion_aut"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView4" />

                <test3.ncxchile.cl.widgets.CustomAutoComplete
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_03_institucion"
                    android:layout_weight="0.51"
                    android:hint="Buscar instituciÃ³n..."
                    android:maxLength="90"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/tableRow2"
                android:layout_alignLeft="@+id/tableRow2"
                android:layout_alignStart="@+id/tableRow2"
                android:id="@+id/tableRow3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cargo_aut"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView5" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_04_cargo"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:maxLength="20"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/tableRow3"
                android:layout_alignLeft="@+id/tableRow3"
                android:layout_alignStart="@+id/tableRow3"
                android:id="@+id/tableRow4">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/unidad_aut"
                    android:layout_marginTop="8dp"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:id="@+id/textView6" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_05_unidadjuris"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:maxLength="20"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:id="@+id/tableRow7"
                android:layout_below="@+id/tableRow5"
                android:layout_alignLeft="@+id/tableRow5"
                android:layout_alignStart="@+id/tableRow5">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/numfunc_aut"
                    android:layout_marginTop="8dp"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:id="@+id/textView7" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1_06_numfunc"
                    android:layout_weight="0.51"
                    android:maxLength="20"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:layout_marginLeft="15dp" />
            </TableRow>


        </LinearLayout>
    </test3.ncxchile.cl.widgets.CustomScrollView>
    <test3.ncxchile.cl.widgets.ScrollArrow
        android:src="@drawable/scroll_arrow_top"
        android:id="@+id/arrow_top1"
        android:layout_height="80dip"
        android:layout_width="80dip"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />
    <test3.ncxchile.cl.widgets.ScrollArrow
        android:src="@drawable/scroll_arrow_bottom"
        android:id="@+id/arrow_bottom1"
        android:layout_height="80dip"
        android:layout_width="80dip"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
    </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
</FrameLayout>