<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:paddingBottom="@dimen/activity_vertical_margin">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/headhead"
        android:layout_gravity="left|top"
        android:id="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/logotipo_policial"
        android:gravity="center"
        android:background="#33b5e5"
        android:layout_below="@id/imageView"
        android:layout_gravity="center" />

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fadeScrollbars="false"
        android:scrollbarStyle="insideOverlay"
        android:id="@+id/scrollView2">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="20dp"
            android:paddingBottom="70dp"
            android:layout_centerHorizontal="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:clickable="true">
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:id="@+id/tableRow">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/fecha_pol"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView" />


                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_01_fechaparte"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_weight="0.51"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow2"
                android:layout_alignLeft="@+id/tableRow2"
                android:layout_alignStart="@+id/tableRow2"
                android:id="@+id/tableRow2">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/num_pol"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView3" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_02_numparte"
                    android:layout_weight="0.51"
                    android:inputType="number"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:layout_below="@+id/tableRow3"
                android:layout_alignLeft="@+id/tableRow3"
                android:id="@+id/tableRow3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:text="@string/unidad_pol"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView5" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_03_numunidadpolicial"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/tableRow2"
                android:layout_alignLeft="@+id/tableRow4"
                android:layout_alignStart="@+id/tableRow4"
                android:id="@+id/tableRow4">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/nue_pol"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:layout_marginTop="8dp"
                    android:id="@+id/textView4" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_04_nue"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_weight="0.51"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/tableRow3"
                android:layout_alignLeft="@+id/tableRow5"
                android:layout_alignStart="@+id/tableRow5"
                android:id="@+id/tableRow5">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/ruc_pol"
                    android:layout_marginTop="8dp"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:id="@+id/textView6" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_05_ruc"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:id="@+id/tableRow8"
                android:layout_below="@+id/tableRow6"
                android:layout_alignLeft="@+id/tableRow6"
                android:layout_alignStart="@+id/tableRow6">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/acta_pol"
                    android:layout_marginTop="8dp"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:id="@+id/textView7" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_06_actaincautacion"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:id="@+id/tableRow7"
                android:layout_below="@+id/tableRow7"
                android:layout_alignLeft="@+id/tableRow7"
                android:layout_alignStart="@+id/tableRow7">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/oficio_pol"
                    android:layout_marginTop="8dp"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:id="@+id/textView8" />

                <test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_07_oficioremisor"
                    android:inputType="textCapWords"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_marginLeft="15dp" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:showDividers="middle"
                android:nestedScrollingEnabled="true"
                android:paddingLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingRight="10dp"
                android:id="@+id/tableRow9"
                android:layout_below="@+id/tableRow8"
                android:layout_alignLeft="@+id/tableRow8"
                android:layout_alignStart="@+id/tableRow8">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tribunal_pol"
                    android:layout_marginTop="8dp"
                    android:textColor="#003076"
                    android:textSize="18dp"
                    android:id="@+id/textView9" />

                <!--test3.ncxchile.cl.widgets.RequiredEditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_08_tribunal"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_marginLeft="15dp" /-->

                <test3.ncxchile.cl.widgets.CustomAutoComplete
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3_08_tribunal"
                    android:layout_weight="0.51"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="5dp"/>
                <fragment
                    android:name="test3.ncxchile.cl.widgets.FragmentButton"
                    android:tag="tribunales"
                    android:id="@+id/fragment_button3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </TableRow>
        </LinearLayout>
      </ScrollView>
    </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
